{
  "name": "in-vue",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "author": "nick@biomousavi.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biomousavi/in-vue.git"
  },
  "homepage": "https://github.com/biomousavi/in-vue",
  "bugs": {
    "url": "https://github.com/biomousavi/in-vue/issues"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/in-vue.umd.cjs",
  "module": "./dist/in-vue.js",
  "types": "./dist/in-vue.d.ts",
  "exports": {
    ".": {
      "import": "./dist/in-vue.js",
      "types": "./dist/in-vue.d.ts",
      "require": "./dist/in-vue.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write lib/"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "typescript": "~5.6.3",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-vue-devtools": "^7.6.5",
    "vue-tsc": "^2.1.10"
  },
  "keywords": [
    "vue",
    "in-vue",
    "vue-in-viewport",
    "async-component",
    "lazy-loading",
    "intersection-observer"
  ]
}